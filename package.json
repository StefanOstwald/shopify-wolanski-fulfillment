{
  "name": "shopify-fulfillment-for-wolanski",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest",
    "build": "rm -rf ./dist && mkdir -p dist && babel src -d ./dist --ignore .test.js && npm run postbuild:copy-files && npm run postbuild:npm-install",
    "build:with-this-node_modules": "mkdir -p dist && babel src -d ./dist --ignore .test.js && npm run postbuild:copy-node_modules-without-sub-node_modules",
    "postbuild:copy-files": "rsync -av package.json ./dist && rsync -av package-lock.json ./dist",
    "postbuild:npm-install": "cd dist && npm install --production && cd ..",
    "postbuild:copy-node_modules-without-sub-node_modules": "rsync -av -L node_modules ./dist --exclude=node_modules/**/node_modules --exclude=*.git",
    "package": "cd dist && zip -r package.zip . && cd ..",
    "upload-lambda": "./bin/deploy --config scripts/deploy/lambdaprod.config.js --package dist/package.zip",
    "deploy": "npm run test && npm run build && npm run package && npm run upload-lambda",
    "lint": "./node_modules/.bin/eslint --fix --quiet --ext .jsx,.js,.json,.html ."
  },
  "engines": {
    "node": ">=8.10"
  },
  "author": "Stefan Ostwald",
  "license": "MIT",
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "basic-ftp": "^2.13.2",
    "bluebird": "^3.4.7",
    "csvtojson": "^2.0.8",
    "dotenv": "^4.0.0",
    "https-proxy-agent": "^2.2.4",
    "i18next": "^8.2.1",
    "json2csv": "^3.11.2",
    "lodash": "^4.17.15",
    "moment": "^2.22.2",
    "moment-timezone": "^0.5.13",
    "promise-ftp": "^1.3.3",
    "request": "^2.80.0",
    "request-promise": "^4.2.1"
  },
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^23.4.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-bluebird": "^1.0.1",
    "babel-preset-env": "^1.6.0",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.23.0",
    "eslint": "^4.14.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-async-await": "0.0.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-json": "^1.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^23.5.0",
    "lambda-tester": "^3.1.0",
    "node-aws-lambda": "^0.1.8",
    "nomnom": "^1.8.1",
    "reflect-metadata": "^0.1.12"
  }
}
